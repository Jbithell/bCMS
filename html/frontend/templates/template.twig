        $('a').smoothScroll({offset: -100});


        var canMakeASuggestion = true; //Only allow a new suggestion to be made after the last one has actually arrived
        $('#searchBox').on('input',function(e){
            if (canMakeASuggestion) {
                canMakeASuggestion = false;
                ajaxCall("searchSuggestions.php", {"searchterm": $(this).val()}, function (result) {
                    canMakeASuggestion = true;
                    if (result.response && result.response.length > 0) {
                        var output = "";
                        $.each(result.response, function (key, value) {
                            output += '<div class="search__suggestion"><h3><a href="' + value['url'] + '">' + value['articlesDrafts_headline'] + '</a></h3><p>' + (value['articlesDrafts_excerpt'] != null ? value['articlesDrafts_excerpt'] : "") + '</p></div>';
                        });
                        $("#searchSuggestions").html(output);
                    }
                });
            }
        });
        $('#searchBox').keypress(function (e) {
            if(e.which ==13) {
                window.location.href = "{{ CONFIG.ROOTFRONTENDURL }}/search?q=" + $("#searchBox").val();
            }
        });
    });
