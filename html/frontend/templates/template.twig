            loadingInner: '<img src="https://nouse.ams3.cdn.digitaloceanspaces.com/site-assets/logo/kingfisher.svg" style="width: 80px;" />',
            browser: [ 'animation-duration', '-webkit-animation-duration'],
            inDuration: 100,
